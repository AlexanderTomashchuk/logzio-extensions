{
    "name": "Logz.io Extensions",
    "version": "1.1",
    "description": "Helps to work with Logz.io",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [{
        "matches": ["https://app.logz.io/*"],
        "js": ["content.js"]
    }],
    "manifest_version": 2,
    "author": "Alexander Tomashchuk"
}